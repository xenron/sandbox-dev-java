      <groupId>net.ensode.glassfishbook</groupId>
      <artifactId>daosessionbean</artifactId>
      <version>1.0</version>
      <type>ejb</type>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-ear-plugin</artifactId>
        <configuration>
          <version>5</version>
        </configuration>
      </plugin>
    </plugins>
  </build>
